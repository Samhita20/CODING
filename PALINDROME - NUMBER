public static void isPalindrome(int n)
    {
        int r;
        int sum=0;
        int temp=n;
        while(n>0)
        {
            r = n%10;
            sum = (sum*10) + r;
            n /= 10;
        }
        if(temp==sum)
            System.out.println("wins");
        else
            System.out.println("loses");
    }
